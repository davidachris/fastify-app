{
  "type": "module",
  "name": "fastify-app",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build && tsc -p test/tsconfig.json && FASTIFY_AUTOLOAD_TYPESCRIPT=1 node --test --experimental-test-coverage --loader ts-node/esm test/**/*.ts",
    "start": "npm run build && fastify start -l info dist/app.js",
    "build": "tsc && npm run minify",
    "minify": "npx esbuild \"dist/**/*.js\" --outdir=dist --minify --platform=node --allow-overwrite",
    "watch:ts": "tsc -w",
    "dev": "npm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
    "release:linux": "docker build --platform linux/amd64 -f Dockerfile.release --target artifact --output ./artifact ."
  },
  "keywords": [],
  "author": "David Christie",
  "license": "UNLICENSED",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/client-ses": "^3.864.0",
    "@aws-sdk/client-sqs": "^3.864.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "@fastify/autoload": "^6.0.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/env": "^5.0.2",
    "@fastify/etag": "^6.0.3",
    "@fastify/helmet": "^13.0.1",
    "@fastify/sensible": "^6.0.0",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.4.0",
    "fastify-plugin": "^5.0.1",
    "mongoose": "^8.17.1"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "esbuild": "^0.25.8",
    "fastify-tsconfig": "^3.0.0",
    "json-schema-to-ts": "^3.1.1",
    "ts-node": "^10.4.0",
    "typescript": "~5.8.2"
  }
}
